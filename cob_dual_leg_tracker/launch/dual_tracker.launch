<launch>
  <!-- dual tracker node -->
  <node pkg="cob_dual_leg_tracker" type="dual_tracker" required="true" name="dual_tracker" args=" $(find cob_dual_leg_tracker)/trained_classificators/forestClassificator.yaml" output="screen"/>

  <!-- relay scans -->
  <node pkg="topic_tools" type="relay" name="relay_front" args="/scan_front /scan" />
  <node pkg="topic_tools" type="relay" name="relay_rear" args="/scan_rear /scan" />
  <node pkg="topic_tools" type="relay" name="relay_back" args="/scan_back /scan" />
  <node pkg="topic_tools" type="relay" name="relay_left" args="/scan_left /scan" />
  <node pkg="topic_tools" type="relay" name="relay_right" args="/scan_right /scan" />
  
</launch>

